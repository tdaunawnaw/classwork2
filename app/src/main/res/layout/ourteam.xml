<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView
        android:id="@+id/ourteam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Our Team"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textSize="30dp"
        android:layout_marginLeft="120dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/act1"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/act1"
        app:layout_constraintTop_toBottomOf="@+id/ourteam"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="15dp"/>

    <ImageView
        android:id="@+id/a1"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/a1"
        app:layout_constraintTop_toBottomOf="@id/ourteam"
        app:layout_constraintLeft_toRightOf="@+id/act1"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="30dp"/>


    <ImageView
        android:id="@+id/act2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/act2"
        app:layout_constraintTop_toBottomOf="@id/ourteam"
        app:layout_constraintLeft_toRightOf="@+id/a1"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="20dp"/>

    <TextView
        android:id="@+id/admas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adams G"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/act1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="30dp"/>

    <TextView
        android:id="@+id/officer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Executive Officer"
        android:textSize="15dp"
        android:layout_marginLeft="15dp"
        app:layout_constraintTop_toBottomOf="@id/admas"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/betty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:text="Betty L"
        app:layout_constraintTop_toBottomOf="@id/a1"
        app:layout_constraintLeft_toRightOf="@id/admas"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="75dp"/>

    <TextView
        android:id="@+id/marketing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:text="Marketing"
        app:layout_constraintTop_toBottomOf="@id/betty"
        app:layout_constraintLeft_toRightOf="@id/officer"
        android:layout_marginLeft="25dp"/>

    <TextView
        android:id="@+id/robert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Roberts"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@+id/act2"
        app:layout_constraintLeft_toRightOf="@id/betty"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="60dp"
        app:layout_constraintRight_toRightOf="parent"/>

    <TextView
        android:id="@+id/analysis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Business Analysis"
        android:layout_marginLeft="30dp"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@+id/robert"
        app:layout_constraintLeft_toRightOf="@id/marketing"
        app:layout_constraintRight_toRightOf="parent"/>

    <ImageView
        android:id="@+id/act3"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/act3"
        app:layout_constraintTop_toBottomOf="@id/officer"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="15dp"/>

    <ImageView
        android:id="@+id/act4"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/act4"
        app:layout_constraintTop_toBottomOf="@id/officer"
        app:layout_constraintLeft_toRightOf="@id/act3"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="30dp"/>

    <ImageView
        android:id="@+id/a2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/a2"
        app:layout_constraintTop_toBottomOf="@id/officer"
      app:layout_constraintLeft_toRightOf="@id/act4"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"/>

    <TextView
        android:id="@+id/miller"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Miller W"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/act4"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="35dp"/>

    <TextView
        android:id="@+id/designer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UX Designer"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/miller"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="20dp"/>

    <TextView
        android:id="@+id/john"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kevin John"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/act4"
        app:layout_constraintLeft_toRightOf="@id/miller"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="70dp"/>

    <TextView
        android:id="@+id/web"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Web Developer"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/john"
        app:layout_constraintLeft_toRightOf="@id/miller"
        android:layout_marginLeft="60dp"/>

    <TextView
        android:id="@+id/laura"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Laura M"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/a2"
        app:layout_constraintLeft_toRightOf="@id/john"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="60dp"/>


    <TextView
        android:id="@+id/mobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mobile Dev"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@id/laura"
        app:layout_constraintLeft_toRightOf="@id/web"
        android:layout_marginLeft="30dp"/>

    <TextView
        android:id="@+id/mission"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mission"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@id/web"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="140dp"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="406dp"
        android:layout_height="237dp"
        android:layout_marginTop="28dp"
        android:text="@string/ourteam"
        app:layout_constraintTop_toBottomOf="@id/mission"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>